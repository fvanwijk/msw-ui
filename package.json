{
  "version": "0.6.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build && cd example-angular && npm run build msw-ui-angular && npm run copy-lib",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "msw-ui",
  "author": "Frank van Wijk <info@frankvanwijk.nl>",
  "module": "dist/msw-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/msw-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/msw-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "msw": ">=0.25",
    "vue": "^3.0.0",
    "@angular/common": ">=9",
    "@angular/core": ">=9",
    "@angular/router": ">=9"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "5.0.1",
    "husky": "7.0.1",
    "size-limit": "5.0.1",
    "tsdx": "0.14.1",
    "tslib": "2.3.0",
    "typescript": "4.3.5",
    "yorkie": "2.0.0"
  }
}
